{
    "routes" : [
        {
            "rule" : "/",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "index"
            }
        },
        {
            "rule" : "/news/{nodeIdentifier:[a-z]+}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "newslist"
            }
        },
        {
            "rule" : "/news/{nodeIdentifier:[a-z]+}/{pageid:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "newslist"
            }
        },
        {
            "rule" : "/404.html",
            "option" : {
                "module" : "Site",
                "controller" : "Exception",
                "action" : "notfind"
            }
        },
        {
            "rule" : "/news/{newsid:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "news"
            }
        },
        {
            "key" : "SiteFrontApiEntry",
            "rule" : "\/front-api-entry",
            "option" : {
                "module" : "Site",
                "controller" : "FrontApiEntry",
                "action" : "index"
            }
        },
        {
            "rule" : "/about.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "about"
            }
        },
        {
            "rule" : "/productlist/{pageId:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "productlist"
            }
        }
    ],
    "hostName" : true,
    "tplMap" : {
        "site/index" : "企业网站/首页.phtml",
        "site/404" : "404.phtml",
        "site/about" : "企业网站/关于我们.phtml",
        "site/productlist" : "企业网站/产品列表.phtml"
    },
    "front_api_map" : {
        "Utils" : [
            "\\SiteFrontApi\\Utils",
            1
        ]
    }
}