{
    "routes" : [
        {
            "rule" : "/",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "index"
            }
        },
        {
            "rule" : "/{nodeIdentifier:[a-z]+}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "newslist"
            }
        },
        {
            "rule" : "/{nodeIdentifier:[a-z]+}/{pageid:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "newslist"
            }
        },
        {
            "rule" : "/404.html",
            "option" : {
                "module" : "Site",
                "controller" : "Exception",
                "action" : "pagenotexist"
            }
        },
        {
            "rule" : "/news/{newsid:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "news"
            }
        },
        {
            "key" : "SiteFrontApiEntry",
            "rule" : "\/front-api-entry",
            "option" : {
                "module" : "Site",
                "controller" : "FrontApiEntry",
                "action" : "index"
            }
        },
        {
            "rule" : "/about.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "about"
            }
        },
        {
            "rule" : "/search.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "search"
            }
        },
        {
            "rule" : "/caselist/{pageId:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "caselist"
            }
        },
        {
            "rule" : "/casedetail/{id:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "casedetail"
            }
        },
        {
            "rule" : "/productlist/{pageId:[1-9][0-9]*}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "productlist"
            }
        },
        {
            "key" : "Item",
            "rule" : "/item/{number:[ZC]{2}[0-9]{16}}.html",
            "option" : {
                "module" : "Site",
                "controller" : "Index",
                "action" : "product"
            }
        }
    ],
    "hostName" : true,
    "tplMap" : {
        "site/index" : "企业网站/首页.phtml",
        "site/404" : "企业网站\/404.phtml",
        "site/about" : "企业网站/关于我们.phtml",
        "site/search" : "企业网站/搜索页.phtml",
        "site/productlist" : "企业网站/产品列表.phtml",
        "site/caselist" : "企业网站/案例列表.phtml",
        "site/casedetail" : "企业网站/案例详情页.phtml",
        "site/product" : "企业网站/产品内容.phtml"
    },
    "front_api_map" : {
        "Utils" : [
            "\\SiteFrontApi\\Utils",
            1
        ]
    }
}